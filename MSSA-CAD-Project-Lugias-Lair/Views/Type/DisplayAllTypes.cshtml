@model IEnumerable<PokemonType>
@{
    ViewBag.Title = "Type Listing";
}
    <h1 class="text-center">Pokemon Types</h1>
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Type Number</th>
                <th scope="col">Type Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var t in Model)
            {
                <tr>
                    <td scope="row">@t.TypeNumber</td>
                    <td>@t.TypeName</td>
                </tr>
            }
        </tbody>
    </table>
</div>
<footer class="align-content-center">
    <a class="btn btn-primary align-content-center" asp-controller="Home" asp-action="ContentHub">Return to Content Hub</a>
</footer>
